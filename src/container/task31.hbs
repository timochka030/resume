 <div class="row">
    <section class="col-12">
        {{#with navigation}}
            {{#each links}} 
                <a class="btn btn-link col-2" href="{{href}}">{{text}}</a>
            {{/each}}

            {{#with button}} 
                <a class="btn btn-primary col-2" href="{{href}}">{{text}}</a>
            {{/with}}
        {{/with}}
    </section>

    <hr>
    <br><br><br>

    <section class="col-12">
        {{#with header}}
            <h1>{{title}}</h1>
            <p>{{description}}</p>
            {{#with button}}
                <a class="btn btn-primary col-2" href="{{link}}">{{text}}</a>
            {{/with}}
        {{/with}}
    </section>
    <hr>
<br><br><br>
    <section class="col-12">
        <div class="row gy-1">
            {{#with city}}
                <h2>{{title}}</h2>
                {{#each cards}}
                    <div class="card col-5">
                        <div class="card-body">
                            {{#with picture}}
                                <img class="card-img-top" src="{{src}}" alt="{{alt}}">
                            {{/with}}
                                    
                            <h3 class="card-title">{{title}}</h3>
                            <p>{{description}}</p>
                            {{#with button}} 
                                <a class="btn btn-primary col-2" href="{{href}}">{{text}}</a>
                            {{/with}}
                        </div>
                    </div>
                {{/each}}
            {{/with}}
        </div>
    <br><br><br>
        {{#with winner}}
            <h2>{{title}}</h2>
                <table class="table text-center">
                    {{#with table}}
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>name</th>
                                <th>surname</th>
                                <th>nickname</th>
                            </tr>
                        </thead>
                        {{#each user}}
                            <tbody>
                                <tr>
                                    <th>{{place}}</th>
                                    <td>{{name}}</td>
                                    <td>{{surname}}</td>
                                    <td>{{nickname}}</td>
                                </tr>
                            </tbody>
                        {{/each}}
                    {{/with}}
                </table>
        {{/with}}
<br><br><br>
        {{#with gallery}}
            <div class="col-12">
                <h2>{{title}}</h2>
                <div class="row gx-2 gy-3">
                <div class="col-3">
                {{#each cards}}
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title">{{name}}</h5>
                            </div>
                            <ul class="list-group">      
                                {{#each items}}
                                    <li class="list-group-item">{{this}}</li>
                                {{/each}}
                            </ul>
                        </div>
                {{/each}}
                </div>
            </div>
        {{/with}}
<hr>
        {{#with relaxion}}
            <div class="display-3">{{title}}</div>
            <div class='row  gx-2 gy-3'>
                {{#each cards}}
                    <section class='col-5'>
                        <div class='card text-bg-secondary'>
                            <div class='card-header'>{{header}}</div><br />
                            <div class='card-title'><h3>{{title}}</h3></div><br />
                            <p>{{description}}</p>
                        </div>
                    </section>
                {{/each}}
            </div>
        {{/with}}
<hr>
    
                {{#with rest}}
                <div class="row gx-2 gy-3">
                    {{#each cards}}
                        <section class='col-6'>
                            <div class='card text-bg-secondary'>
                                <div class='card-header'>{{header}}</div><br />
                                <div class='card-title'><h3>{{title}}</h3></div><br />
                                <p>{{description}}</p>
                                <div class="btn btn-primary col-2">{{button}}</div>
                                <div class='card-footer text-center'>{{footer}}</div>
                            </div>
                        </section>
                    {{/each}}
                </div>
                {{/with}}
<hr>

                {{#with proposition}}
                    <div class="display-2">{{title}}</div>
                    <div class='row  gx-2 gy-3'>
                        {{#each card}}
                            <section class='col-5'>
                                <div class='card text-bg-warning'>
                                    <div class='card-header'>{{header}}</div><br />
                                    <div class='card-title'><h3>{{title}}</h3></div><br />
                                    <p>{{description}}</p>
                                </div>
                            </section>
                        {{/each}}
                    </div>
                {{/with}}
<hr>
                {{#with footer}}
                    <h5>{{title}}</h5>
                    <div class="col-12">
                        <div>
                            <div class='row  gx-2 gy-3'>
                                <div class="col-3">
                                    {{#each navigation}}
                                        <a class="btn btn-link" href="{{href}}">{{text}}</a><br><br>
                                    {{/each}}
                                </div>

                                <div class="col-3">
                                    {{#each socialMedia}}
                                        <a class="btn btn-outline-dark" href="{{link}}">{{name}}</a><br><br>
                                    {{/each}}
                                </div>

                                <div class="col-3">
                                    {{#each partners}}
                                        <a class="btn btn-outline-info" href="{{link}}">{{name}}</a><br><br>
                                    {{/each}}
                                </div>
                            </div>
                        </div>
                    </div>
                {{/with}}
        </div>
    </section>
</div>