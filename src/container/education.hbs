<div class="p-5">
    <p>Education page in progress...</p>
    {{#with main}}
        <main>
            <section>
                {{> title data="My educationss" big=true}}
                <ul>
                    {{#each educations}}
                        <li>
                            {{name}} 
                            {{#unless isEnd includeZero=true}} (ще в процесі) {{/unless}}
                        </li>
                    {{/each}}
                </ul>
            </section>
            

            <section>
                {{title}}
                <ul>
                    {{#each certificates}}
                        <li>{{name}} ({{id}})</li>
                    {{/each}}
                </ul>
            </section>
        </main>
    {{/with}}
</div>